parameters:
    <% node['symfony']['parameters'].each do |key1,value1| -%>
    <% if value1.is_a?(String) || value1.is_a?(Integer) || value1.is_a?(Float) -%>
    <%= key1 %>: <%= value1 %>
    <% else value1.is_a?(Object) || value1.is_a?(Array) -%>
    <%= key1 %>:
        <% value1.each do |key2,value2| -%>
        <% if value2.is_a?(String) || value2.is_a?(Integer) || value1.is_a?(Float) -%>
        <%= key2 %>: <%= value2 %>
        <% else value2.is_a?(Object) || value2.is_a?(Array) -%>
        <%= key2 %>:
            <% value2.each do |key3,value3| -%>
            <% if value3.is_a?(String) || value3.is_a?(Integer) || value1.is_a?(Float) -%>
            <%= key3 %>: <%= value3 %>
            <% else value3.is_a?(Object) || value3.is_a?(Array) -%>
            <%= key3 %>:
                <% value3.each do |key4,value4| -%>
                <%= key4 %>: <%= value4 %>
                <% end # end each
            end # end if
            end # end each
        end # end if
        end # end each
    end # end if
    end # end each -%>

